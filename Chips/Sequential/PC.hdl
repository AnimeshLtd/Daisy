CHIP PC {
  IN in[16], inc, load, reset;
  OUT out[16];

  PARTS:
  Inc16(in=feedback, out=incremented);
  Mux16(a=in, b=incremented, sel=inc, out=input);
  Mux16(a=input, b=false, sel=reset, out=registerInput);
  Mux16(a=load, b=true, sel=reset, out=loadRegister);
  Register(in=registerInput, load=loadRegister, out=feedback, out=out);
}